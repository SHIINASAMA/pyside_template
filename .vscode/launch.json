{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug App",
            "type": "debugpy",
            "request": "launch",
            "module": "app",
            "envFile": "${workspaceFolder}/.env",
            "preLaunchTask": "Build Resources",
        },
        {
            "name": "Run Test",
            "type": "debugpy",
            "request": "launch",
            "module": "pytest",
            "args": [
                "app"
            ],
            "envFile": "${workspaceFolder}/.env",
            "preLaunchTask": "Build Resources",
        },
    ],
}